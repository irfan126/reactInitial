    "bootstrap": "^4.3.1",
    "bootstrap-daterangepicker": "^2.1.30",
    "camel-case": "^3.0.0",
    "express": "^4.16.4",
    "focus-trap-react": "^6.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.19",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.14.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
